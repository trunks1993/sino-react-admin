{
  "name": "sino-react-admin",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/config.dev.ts",
    "start:mock": "cross-env NODE_ENV=development MOCK=ture webpack-dev-server --config ./config/config.dev.ts",
    "build": "cross-env NODE_ENV=production webpack --config ./config/config.prod.ts",
    "page": "ts-node ./scripts/page-generator.ts",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint"
    }
  },
  "lint-staged": {
    "src/**/*.{tsx, ts, css}": [
      "yarn lint",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie >= 9",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime-corejs3": "^7.17.9",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.17.0",
    "@types/inquirer": "^8.2.1",
    "@types/js-cookie": "^3.0.1",
    "@types/js-md5": "^0.4.3",
    "@types/lodash-es": "^4.17.6",
    "@types/mkdirp": "^1.0.2",
    "@types/mockjs": "^1.0.6",
    "@types/prettier": "^2.6.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/signale": "^1.4.4",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-loader": "^8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "^7.29.4",
    "fork-ts-checker-webpack-plugin": "^7.2.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "inquirer": "^8.2.2",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.4.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.3",
    "signale": "^1.4.0",
    "slash2": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "umi-mock": "2.1.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "immer": "^9.0.12",
    "js-base64": "^3.7.2",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "umi-request": "^1.4.0"
